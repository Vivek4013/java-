abstract class Bank
{
    abstract int get rate of interest ();
}
class SBI extend Bank
{
    int get Rate of interest(){return 7;}
    
}
class PNB extend Bank{
    int get Rate of interest(){return 8;}
}

class text Bank
{
Public Static void main(String args[]){
    Bank b;
    b=new SBI();
    System.out.println("Rate of interest is"+b.getRateof interest()+"%");
    b=new PNB();
    System.out.println("Rate of interst is"+b.get Rate of interest()+"%");
}
}
